<head>
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta http-equiv="content-type" content="text/html; charset=utf-8" />
    <meta name="author" content="M_Adnan" />
    <!-- Document Title -->
    <title>Resume | @ViewBag.Title</title>

    <!-- StyleSheets -->
    <link rel="stylesheet" href="~/css/ionicons.min.css">
    <link rel="stylesheet" href="~/css/bootstrap/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/main.css">
    <link rel="stylesheet" href="~/css/style.css">
    <link rel="stylesheet" href="~/css/responsive.css">
    <link href="~/lib/toastr.js/toastr.min.css" rel="stylesheet" />
    <script src="https://kit.fontawesome.com/1154e1d6ee.js" crossorigin="anonymous"></script>


    <!-- FontsOnline -->

    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">

    <link href="https://fonts.googleapis.com/css?family=Raleway:100,200,300,400,500,600,700,800,900" rel="stylesheet">

    <!-- JavaScripts -->
    <script src="~/js/vendors/modernizr.js"></script>
    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
        <![endif]-->
</head>
<body>
    <!-- Page Wrapper -->
    <div id="wrap">

        <!-- Header -->
        <nav id="cd-lateral-nav">
            <h5 class="tittle">MotiCv</h5>
            <ul class="cd-navigation">
                <li><a asp-controller="Home" asp-action="index">About Me</a></li>
                <li><a asp-controller="Home" asp-action="portfolio">Portfolio</a></li>
                <li><a asp-controller="Blog" asp-action="index">Blog</a></li>
                <li><a asp-controller="Home" asp-action="contact">Contact Me</a></li>
                <li class="item-has-children">
                    <a href="index.html">Pages</a>
                    <ul class="sub-menu">
                        <li><a asp-controller="Blog" asp-action="BlogDetails">Blog Details</a></li>
                        <li><a asp-controller="Resume" asp-action="index">Resume (edit mode)</a></li>
                    </ul>
                </li>
            </ul>
            <h5 class="tittle">Latest Post</h5>
            <ul class="latest-post">

                <!-- Post Small -->
                <li>
                    <div class="media">
                        <div class="media-left"> <a href="#."><img src="~/images/small-img.jpg" alt=""></a></div>
                        <div class="media-body">
                            <a href="#.">
                                Hello Itaewon
                                Project
                            </a>
                            <p class="font-crimson font-italic">Webdesign</p>
                        </div>
                    </div>
                </li>
                <!-- Post Small -->
                <li>
                    <div class="media">
                        <div class="media-left"> <a href="#."><img src="~/images/small-img-1.jpg" alt=""></a></div>
                        <div class="media-body">
                            <a href="#.">Casual in grey</a>
                            <p class="font-crimson font-italic">Webdesign</p>
                        </div>
                    </div>
                </li>
                <!-- Post Small -->
                <li>
                    <div class="media">
                        <div class="media-left"> <a href="#."><img src="~/images/small-img-2.jpg" alt=""></a></div>
                        <div class="media-body">
                            <a href="#.">Transittions</a>
                            <p class="font-crimson font-italic">Webdesign</p>
                        </div>
                    </div>
                </li>
            </ul>
        </nav>

        <!-- End Header -->
        <!-- Content -->
        <main class="cd-main-content">
            <div id="content" class="bg">
                <div class="resume">
                    <div class="container">

                        <!-- TOP HEAD -->
                        <div class="top-head">
                            <div class="row">
                                <div class="col-sm-6">
                                    <h4>Resume </h4>
                                    <a asp-controller="Resume" asp-action="Index" asp-area="Admin">Edit Resume</a>
                                </div>
                                <div class="col-sm-6"> <a id="cd-menu-trigger" href="#0"><span class="cd-menu-icon"></span></a> </div>
                            </div>
                        </div>



                        <!-- NAV LINKS -->
                        <nav class="margin-bottom-30">
                            <!-- Brand and toggle get grouped for better mobile display -->
                            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#nav-tabis" aria-expanded="false">
                                <span class="tittle">MENU</span>
                                <span class="fa fa-navicon icon-nav"></span>
                            </button>
                            <!-- Collect the nav links, forms, and other content for toggling -->
                            <div class="collapse navbar-collapse" id="nav-tabis">
                                <ul class="isop-filter nav nav-pills">
                                    <li id="nav-about"><a asp-controller="Home" asp-action="index"><i class="icon-user"></i> ABOUT ME</a></li>
                                    <li id="nav-resume"><a asp-controller="Home" asp-action="resumeMain"><i class="icon-book-open"></i>RESUME</a></li>
                                    <li id="nav-portfolio"><a asp-controller="Home" asp-action="portfolio"><i class="icon-rocket"></i>PORTFOLIO</a></li>
                                    <li id="nav-blog"><a asp-controller="Blog" asp-action="index"><i class="icon-note"></i>BLOG</a></li>
                                    <li id="nav-contact"><a asp-controller="Home" asp-action="contact"> <i class="icon-pencil"></i>CONTACT ME</a></li>
                                </ul>
                            </div>
                        </nav>

                     

                        <!-- NAV LINKS END -->
                        <!-- Resume -->
                        <div class="row">

                            <!-- Sidebar -->
                            <div class="col-md-4">
                                <div class="side-bar">

                                    <!-- Professional Details -->
                                    <h5 class="tittle">Professional Details</h5>
                                    <img src="~/images/avatar.jpg" class="img-responsive" alt="">
                                    <ul class="personal-info">
                                        <li>
                                            <p><span>Name</span></p>
                                        </li>
                                        <li>
                                            <p><span>Age</span></p>
                                        </li>
                                        <li>
                                            <p><span>Location</span></p>
                                        </li>
                                        <li>
                                            <p><span>Experience</span></p>
                                        </li>
                                        <li>
                                            <p><span>Degree</span></p>
                                        </li>
                                        <li>
                                            <p><span>Career Level</span></p>
                                        </li>
                                        <li>
                                            <p><span>Phone</span></p>
                                        </li>
                                        <li>
                                            <p><span>FAX</span> </p>
                                        </li>
                                        <li>
                                            <p><span>E-mail</span> <a href="#"></a></p>
                                        </li>
                                    </ul>

                                    <!-- Attachments -->
                                    <h5 class="tittle">Attachments</h5>
                                    <div class="attach bor-btm padding-25">
                                        <ul>
                                            <li>
                                                <p><img src="~/images/pdf-icon.jpg" alt=""> Curriculum-Vitae.pdf <a href="#."><i class="fa fa-cloud-download"></i></a> <a href="#."><i class="fa fa-eye"></i></a></p>
                                            </li>
                                            <li>
                                                <p><img src="~/images/word-icon.jpg" alt=""> COE.docx <a href="#."><i class="fa fa-cloud-download"></i></a> <a href="#."><i class="fa fa-eye"></i></a></p>
                                            </li>
                                        </ul>
                                    </div>

                                    <!-- Social Profiles -->
                                    <h5 class="tittle">Social Profiles</h5>
                                    <div class="social-icon bor-btm padding-25">
                                        <ul>
                                            <li> <a href="#."> <i class="fa fa-facebook"></i></a> </li>
                                            <li> <a href="#."> <i class="fa fa-google"></i></a> </li>
                                            <li> <a href="#."> <i class="fa fa-twitter"></i></a> </li>
                                            <li> <a href="#."> <i class="fa fa-linkedin"></i></a> </li>
                                            <li> <a href="#."> <i class="fa fa-skype"></i></a> </li>
                                        </ul>
                                    </div>

                                    <!-- Contact Me -->
                                    <h5 class="tittle">Contact Me</h5>
                                    <div class="contact padding-25">
                                        <!-- Success Msg -->
                                        <div id="contact_message" class="success-msg"> <i class="fa fa-paper-plane-o"></i>Thank You. Your Message has been Submitted</div>

                                        <!-- FORM -->
                                        <form id="contactMe" class="contact-form">
                                            <ul class="row">
                                                <li class="col-sm-12">
                                                    <label>
                                                        <input type="text" class="form-control" name="email" id="email" placeholder="Email">
                                                    </label>
                                                </li>
                                                <li class="col-sm-12">
                                                    <label>
                                                        <input type="text" class="form-control" name="subject" id="company" placeholder="Subject">
                                                    </label>
                                                </li>
                                                <li class="col-sm-12">
                                                    <label>
                                                        <textarea class="form-control" name="message" id="message" rows="5" placeholder="Message"></textarea>
                                                    </label>
                                                </li>
                                                <li class="col-sm-12">
                                                    <button type="submit" value="submit" id="btn_submit" onClick="proceed();">Send Message</button>
                                                </li>
                                            </ul>
                                        </form>
                                    </div>
                                </div>
                            </div>
                            @RenderBody()

                        </div>
                    </div>
                </div>
            </div>
        </main>
        <!-- End Content -->
        <!-- Footer -->
        <footer class="footer">
            <div class="rights">
                <p>© Copyright 2016 MotiCV. All right reserved.</p>
            </div>
        </footer>
        <!-- End Footer -->

    </div>

    <!-- End Page Wrapper -->
    <!-- JavaScripts -->
    <script src="~/js/vendors/jquery/jquery.min.js"></script>
    <script src="~/js/vendors/bootstrap.min.js"></script>
    <script src="~/js/vendors/owl.carousel.min.js"></script>
    <script src="~/js/vendors/jquery.isotope.min.js"></script>
    <script src="~/js/main.js"></script>
    <script src="~/lib/toastr.js/toastr.min.js"></script>
    <script src="~/js/overrides/toastr.override.js"></script>


    <!-- Begin Map Script-->
    <script src="https://maps.googleapis.com/maps/api/js?v=3.exp"></script>
    <script src="~/js/vendors/map.js"></script>

    @await RenderSectionAsync("jsadd")
   
    
</body>
</html>
